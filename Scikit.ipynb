   "source": [
    "from sklearn import datasets, svm\n",
    "\n",
    "svc = svm.SVC(gamma=0.001, C=100.)\n",
    "#print(svc)\n",
    "\n",
    "iris = datasets.load_iris()\n",
    "#print(iris.DESCR)\n",
    "x, y = iris.data, iris.target\n",
    "svc.fit(x,y)\n",
    "\n",
    "import pickle\n",
    "s = pickle.dumps(svc)\n",
    "clf2 = pickle.loads(s)\n",
    "clf2.predict(x[0:1])\n",
    "\n",
    "y[0]"
   ]
